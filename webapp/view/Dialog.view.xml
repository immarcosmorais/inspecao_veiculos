<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.formInspecaoDeVeiculos.controller.Dialog2" xmlns="sap.m">
    <Dialog horizontalScrolling="true" showHeader="true" state="Warning" stretch="false" title="" type="Message" verticalScrolling="true" class="sapUiContentPadding">
        <content>
            <Text text="Deseja deletar este cadastro de veículo?" width="100%" maxLines="1" wrapping="false" textAlign="Center" textDirection="Inherit" visible="true"/>
        </content>
        <beginButton/>
        <buttons>
            <Button text="Sim" type="Accept" icon="sap-icon://accept" iconFirst="true" width="auto" enabled="true" visible="true" tooltip="" iconDensityAware="false" class="sapUiTinyMargin" press="_onButtonPress"/>
            <Button text="Não" type="Reject" icon="sap-icon://negative" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" class="sapUiTinyMargin" press="_onButtonPress1"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</mvc:View>
