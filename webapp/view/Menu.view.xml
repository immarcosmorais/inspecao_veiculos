							</TileContent>
						</tileContent>
					</GenericTile>
					<GenericTile header="Listar" subheader="Listar vistorias realizadas" frameType="OneByOne" class="sapUiTinyMargin" press="_onGenericTilePress1">
						<tileContent>
							<TileContent footer="" unit="">
								<content>
									<NumericContent value=" " valueColor="Neutral" indicator="None" icon="sap-icon://list"/>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</items>
				<layoutData/>
