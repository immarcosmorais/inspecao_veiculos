<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core"
	controllerName="com.sap.build.standard.formInspecaoDeVeiculos.controller.Conclusao">
	<Page showHeader="true" title="Conclusão" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title>
					<core:Title text="Resultado" level="Auto"/>
				</form:title>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:fields>
									<RadioButtonGroup id="resultadoRb" columns="1" selectedIndex="0" valueState="None" width="auto" editable="true" enabled="true"
										textDirection="Inherit" select="_onRadioButtonGroupSelect">
										<buttons>
											<RadioButton text="Aprovado" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="false"/>
											<RadioButton text="Reprovado" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="false"/>
										</buttons>
									</RadioButtonGroup>
								</form:fields>
							</form:FormElement>
							
							<form:FormElement visible="true">
								<form:label>
									<Label text="Carregamento" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<DatePicker dateValue="{/date}" id="dataInput" valueFormat="yyyyMMdd"/>
									<!--<DatePicker dateValue="{/date}" id="dataInput"/>-->
								</form:fields>
							</form:FormElement>
						
							<form:FormElement visible="true">
								<form:label>
									<Label text="Observações" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<TextArea id="obsInput" growing="true" visible="true" width="100%" maxLength="130"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="auto" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Salvar" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
						press="_inputDados"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>