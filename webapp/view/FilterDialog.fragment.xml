<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem text="Vistoria" key="Vistoria" multiSelect="false">
				<items>
					<ViewSettingsItem text="Placa do veiculo" key="Veiculo-" selected="true"/>
					<ViewSettingsItem text="Numero da Vistoria" key="Id-"/>
					<ViewSettingsItem text="Nome do Motorista" key="Nome-"/>
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem text="Status" key="Status" multiSelect="false">
				<items>
					<ViewSettingsItem text="Todas" key="Status-" selected="true"/>
					<ViewSettingsItem text="Em Aberto" key="Status-A"/>
					<ViewSettingsItem text="Concluido" key="Status-C"/>
					<ViewSettingsItem text="Estornado" key="Status-E"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>